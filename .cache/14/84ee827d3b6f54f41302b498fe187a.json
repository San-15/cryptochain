    const dataDisplay = stringifiedData.length >25 ? \r\n        `${stringifiedData.substring(0,16)}...` : \r\n        stringifiedData;\r\n\r\n        if (this.state.displayTransaction) {\r\n            return (\r\n                <div>\r\n                {\r\n                 data.map(transaction => (\r\n                    <div key={transaction.id}>\r\n                        <hr />\r\n                        <Transaction transaction={transaction}/>    \r\n                    </div>\r\n                    ))\r\n                }\r\n                <br/>\r\n                <Button \r\n                    bsStyle=\"danger\" \r\n                    bsSize=\"small\" \r\n                    onClick={this.toggleTransaction}\r\n                >\r\n                    Show Less\r\n                </Button>\r\n                </div>\r\n            )\r\n        }\r\n        return(\r\n            <div>\r\n            <div>Data: {dataDisplay}</div>\r\n            <Button \r\n            bsStyle=\"danger\" \r\n            bsSize=\"small\" \r\n            onClick={this.toggleTransaction}\r\n            >\r\n                Show More\r\n            </Button>\r\n            </div>\r\n        );\r\n    }\r\n\r\n    render() {\r\n    \r\n\r\n    console.log('this.displayTransaction', this.displayTransaction);\r\n\r\n      const {timestamp,hash} = this.props.block;\r\n     \r\n      \r\n      const hashDisplay = `${hash.substring(0,16)}...`;\r\n\r\n\r\n\r\n      return(\r\n          <div className='Block'>\r\n              <div>Hash : {hashDisplay} </div>\r\n              <div >Timestamp: {new Date(timestamp).toLocaleString()}</div>\r\n              \r\n              {this.displayTransaction}\r\n          </div>\r\n      )\r\n    };\r\n}\r\n\r\nexport default Block;"},"lineCount":null}},"error":null,"hash":"a2fe3796f27906ea8b80099799291098","cacheData":{"env":{}}}