ce":"components/ConductTransaction.js","original":{"line":67,"column":5}},{"name":"Component","generated":{"line":117,"column":2},"source":"components/ConductTransaction.js","original":{"line":6,"column":33}},{"name":"ConductTransaction","generated":{"line":119,"column":15},"source":"components/ConductTransaction.js","original":{"line":71,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from \"react\";\r\nimport {FormGroup,FormControl,Button} from 'react-bootstrap';\r\nimport history from '../history';\r\nimport {Link,withRouter,useHistory} from 'react-router-dom';\r\n\r\nclass ConductTransaction extends Component {\r\n    state = {recipient: '',amount: 0}\r\n\r\n    updateRecipient = event => {\r\n        this.setState({recipient: event.target.value});\r\n    }\r\n    updateAmount = event => {\r\n        this.setState({amount: Number(event.target.value) });\r\n    }\r\n\r\n    ConductTransaction = () => {\r\n        const {recipient,amount} = this.state;\r\n\r\n        fetch(`${document.location.origin}/api/transact`,{\r\n            method: 'POST',\r\n            headers: { 'Content-Type': 'application/json'},\r\n            body: JSON.stringify({recipient,amount})\r\n            \r\n        }).then(response => response.json())\r\n        .then(json => {\r\n            alert(json.message || json.type);\r\n            \r\n            //history.push('/transaction-pool');\r\n        });\r\n    }\r\n\r\n    render() {\r\n        \r\n\r\n        return(\r\n            <div className='ConductTransaction'>\r\n                <Link to='/'>Home</Link>\r\n                <h3>Conduct a Transaction</h3>\r\n                <FormGroup>\r\n                    <FormControl\r\n                    input='text'\r\n                    placeholder='recipient'\r\n                    value={this.state.recipient}\r\n                    onChange={this.updateRecipient}                    \r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <FormControl\r\n                    input='number'\r\n                    placeholder='amount'\r\n                    value={this.state.amount}\r\n                    onChange={this.updateAmount}                    \r\n                    />\r\n                </FormGroup>\r\n                <div>\r\n                    <Button \r\n                    bsStyle=\"danger\" \r\n                    onClick={this.ConductTransaction}\r\n                    >\r\n                        Submit\r\n                    </Button>\r\n                </div>\r\n                <br/>\r\n                <div><Link to='/transaction-pool'>View Transaction Pool Map</Link></div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\n\r\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"904886ac15ba270f5745250e11951362","cacheData":{"env":{}}}